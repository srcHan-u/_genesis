{
  "name": "markup-boilerplate",
  "version": "1.0.0",
  "description": "markup-boilerplate",
  "scripts": {
    "start": "set NODE_ENV=development&&webpack serve --open",
    "dev": "set NODE_ENV=development&&webpack --watch",
    "build": "set NODE_ENV=production&&webpack",
    "prettier": "prettier --write \"**/*.{js,html,scss}\""
  },
  "browserList": [
    "last 2 versions",
    "> 1%",
    "IE 10"
  ],
  "license": "Unlicensed",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "autoprefixer": "^10.4.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-import": "^1.13.1",
    "clean-webpack-plugin": "^4.0.0",
    "css-loader": "^5.2.7",
    "file-loader": "^6.2.0",
    "html-loader": "^3.0.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.0",
    "mini-css-extract-plugin": "^1.6.2",
    "postcss": "^8.4.5",
    "postcss-loader": "^4.3.0",
    "postcss-mixins": "^7.0.3",
    "postcss-nested": "^5.0.6",
    "postcss-variables": "^1.1.1",
    "precss": "^4.0.0",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "sass": "^1.45.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "wepack-cli": "0.0.1-security"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}

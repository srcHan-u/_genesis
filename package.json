    "babel-plugin-import": "^1.13.1",
    "css-loader": "^6.5.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^4.3.0",
    "mini-css-extract-plugin": "^1.6.2",
